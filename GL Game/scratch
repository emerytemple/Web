		<script type="glsl" id="vertex_shader">
			attribute vec2 coordinates;

			void main(void) {
				gl_Position = vec4(coordinates,0.0, 1.0);
			}
		</script>
		<script type="glsl" id="fragment_shader">
			precision mediump float;

			void main(void) {
				gl_FragColor = vec4(0.0, 0.0, 0.0, 0.1);
			}
		</script>
